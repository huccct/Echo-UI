{
  "name": "tass-ui",
  "version": "0.4.4",
  "description": "A high quality UI Toolkit built on Vue.js3+.",
  "main": "index.js",
  "scripts": {
    "test": "pnpm --filter components test",
    "coverage": "pnpm --filter components coverage",
    "dev": "pnpm -C examples dev --open",
    "build:components": "pnpm --filter components build",
    "del-mod": "rimraf node_modules packages/components/node_modules",
    "build:theme": "pnpm run -C packages/theme-chalk build",
    "prepare": "husky install",
    "lint:fix": "eslint . --fix",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --write .",
    "lint:ls-lint": "ls-lint",
    "lint:staged": "lint-staged",
    "lint:css": "stylelint 'packages/theme-chalk/src/**/*.scss' --fix --custom-syntax postcss-scss",
    "postinstall": "npx husky install",
    "commit": "git add . && git-cz",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "docs:preview": "vitepress preview docs --port 8080"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@ls-lint/ls-lint": "^1.11.2",
    "@types/node": "^18.11.19",
    "@types/sass": "^1.43.1",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.2.10",
    "c8": "^7.12.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "happy-dom": "^8.1.5",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "sass": "^1.55.0",
    "stylelint": "^14.16.1",
    "stylelint-scss": "^4.3.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-dts": "^1.4.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vitepress": "1.0.0-alpha.45",
    "vitepress-theme-demoblock": "^2.0.2",
    "vitest": "^0.27.3",
    "vue": "^3.2.45",
    "mitt": "^3.0.0",
    "vue-router": "^4.1.6",
    "vue3-eventbus": "^2.0.0"
  },
  "dependencies": {
    "@jridgewell/sourcemap-codec": "^1.4.14",
    "@tass-ui/components": "workspace:^1.0.0",
    "@tass-ui/constants": "workspace:^1.0.0",
    "@tass-ui/eslint-config": "workspace:^1.0.0",
    "@tass-ui/theme-chalk": "workspace:^1.0.0",
    "@tass-ui/utils": "workspace:^1.0.0"
  }
}
